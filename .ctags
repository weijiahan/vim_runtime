--langdef=fidl
--langmap=fidl:.fidl
--regex-fidl=/^[ \t]*interface[ \t]+([a-zA-Z0-9_:]+)/\1/i,interface/
--regex-fidl=/^[ \t]*method[ \t]+([a-zA-Z0-9_:]+)/\1/m,method/
--regex-fidl=/^[ \t]*broadcast[ \t]+([a-zA-Z0-9_:]+)/\1/b,broadcast/
--regex-fidl=/^[ \t]*enumeration[ \t]+([a-zA-Z0-9_:]+)/\1/e,enumeration/
--regex-fidl=/^[ \t]*struct[ \t]+([a-zA-Z0-9_:]+)/\1/s,struct/
--regex-fidl=/^[ \t]*union[ \t]+([a-zA-Z0-9_:]+)/\1/u,union/
--regex-fidl=/^[ \t]*map[ \t]+([a-zA-Z0-9_:]+)/\1/a,map/
--regex-fidl=/^[ \t]*typeCollection[ \t]+([a-zA-Z0-9_:]+)/\1/t,typeCollection/
--regex-fidl=/^[ \t]*typedef[ \t]+([a-zA-Z0-9_:]+)/\1/y,typedef/
